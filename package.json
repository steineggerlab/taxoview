{
	"name": "taxoview",
	"author": "SunJae Lee <snjlee58@gmail.com>",
	"productName": "Taxo Sankey",
	"description": "A Vue.js plugin for creating interactive and customizable Sankey diagrams using D3, designed to visualize Kraken report data effectively.",
	"version": "1.0.1",
	"main": "./dist/taxoview.umd.js",
	"module": "./dist/taxoview.es.js",
	"exports": {
		".": {
			"import": "./dist/taxoview.es.js",
			"require": "./dist/taxoview.umd.js"
		},
		"./dist/style.css": "./dist/style.css"
	},
	"files": [ "dist" ],
	"scripts": {
		"build": "vite build -c vite.config.js",
		"buildchart": "vite build -c vite.config.chart.js"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.6.2",
		"vite": "^4.5.9",
		"vue": "^3.0.0"
	},
	"dependencies": {
		"d3": "^7.9.0",
		"d3-sankey": "^0.12.3"
	}
}
